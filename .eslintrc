{
  "env": {
    "es6": true,
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
